page{background-color:#ededed}.header{padding:20rpx 30rpx;box-sizing:border-box;background-color:#fff;margin-bottom:20rpx}.header .right{width:calc(100% - 100rpx);padding-left:30rpx}.header .icon{width:100%;justify-content:space-between;font-size:26rpx;color:#b3b3b3;margin-top:16rpx}.header .name{font-weight:700}.header .image{width:100rpx;height:100rpx;border-radius:10rpx}.list_item{padding:30rpx;box-sizing:border-box;justify-content:space-between;background-color:#fff;border-bottom:1rpx solid #ededed}.list_item .item{font-size:28rpx}.list_item .item_icon{margin-right:20rpx;font-size:40rpx}.top20{margin-top:20rpx;border-bottom:none}
